<div id="profile">
  <div class="inside">
    <img class="tile" ngf-src="users.user.imageSRC">
    <p>{{users.user.username}}</p>
    <button ui-sref-active="active" ui-sref="edit">edit</button>
  </div>
</div>  
 
<div class="box-friends container">
  <div class="col-xs-6 col-md-4 friend" ng-repeat="friend in users.user.friends">
    <img class="tile" ngf-src="'https://s3-eu-west-1.amazonaws.com/wdi-london18/' + friend.avatar">
    <div class="outside">
      {{friend.username}}
      <div class="connect">
        <p class="inline">{{ friend.friends.length }}  <i class="fa fa-users" aria-hidden="true"></i></p>
        <a ng-click="users.deleteConnection(friend._id)">delete</a>
      </div>
    </div>  
  </div>     
</div>