<div id="profile">
  <div class="inside">
    <img class="tile" ngf-src="users.user.imageSRC">
    <p>{{users.user.username}}</p>
    <button ui-sref-active="active" ui-sref="edit">edit</button>
  </div>
</div>  
<div class="container box-friends"> 
  <div class="col-xs-6 col-md-4 friend" ng-repeat="friend in users.user.friends">
    <div class="inside">
      <img class="tile" ngf-src="friend.imageSRC">
      <p>{{ friend.username }}</p>
      <button ng-click="users.deleteConnection(friend._id)">delete</button>
    </div>  
  </div> 
</div> 
