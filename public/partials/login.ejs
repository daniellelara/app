<div class="container" id="login">
  <div class="inside">
    <h3 class="item">login</h3>
    <form ng-submit="users.login()" novalidate>
      <input class="input" type="text" name="email" placeholder="Email" ng-model="users.currentUser.email">
      <input class="input" type="password" name="password" placeholder="Password" ng-model="users.currentUser.password">
      <button type="submit" class="item"><i class="fa fa-sign-in" aria-hidden="true"></i></button>
    </form>
  </div>  
</div>